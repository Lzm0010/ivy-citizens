<%= form_for @customer do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>
    <br/>

    <%= f.label :email %>
    <%= f.text_field :email %>
    <br/>

    <%= f.label :royalty %>
    <%= f.number_field :royalty %>
    <br/>

    <%= f.label :wholesale_margin %>
    <%= f.number_field :wholesale_margin %>
    <br/>

    <%= f.submit %>
<% end %>